@model ShapeViewModel<AuditTrailIndexOptions>
@{
    var term = Model.Value.FilterResult.FirstOrDefault(x => x.TermName == "id");
}

<h4 class="card-title">@T["Id"]</h4>
<pre class="mb-3">@(@term != null ? @term.ToString() : "id:...")</pre>
<p>@T["Filters on a correlation id."]</p>
<div class="d-block text-right">
    <span>
        <i class="fas fa-sm fa-minus text-primary" aria-hidden="true"></i>
    </span>
</div>
